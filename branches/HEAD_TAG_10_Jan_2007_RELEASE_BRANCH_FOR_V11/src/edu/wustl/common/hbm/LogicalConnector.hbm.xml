<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
        name="edu.wustl.common.querysuite.queryobject.impl.LogicalConnector"
        table="QUERY_LOGICAL_CONNECTOR"
        dynamic-update="false"
        dynamic-insert="false"
    >
        <cache usage="read-write" />

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">LOGICAL_CONNECTOR_SEQ</param>
            </generator>
        </id>

        <property
            name="operatorString"
            type="string"
            update="true"
            insert="true"
            column="LOGICAL_OPERATOR"
        />

        <property
            name="nestingNumber"
            type="integer"
            update="true"
            insert="true"
            column="NESTING_NUMBER"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-LogicalConnector.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
